{
    "manifest_version": 3,
    "name": "YouTube Speed Adjuster",
    "version": "1.0",
    "description": "Adjust video playback speed on YouTube.",
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/watch*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": ["activeTab", "storage"]
  }
  